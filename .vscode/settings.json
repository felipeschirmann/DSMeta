{
    "java.compile.nullAnalysis.mode": "automatic",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".vscode": false,
        "frontend": true
    },
    "explorerExclude.backup": {},
