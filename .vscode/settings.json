        "statusBar.background": "#2747d2",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#4e69de",
        "statusBarItem.remoteBackground": "#2747d2",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#2747d2",
